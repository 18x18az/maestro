{
  "name": "@18x18az/maestro",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "license": "UNLICENSED",
  "repository": "https://github.com/18x18az/maestro",
  "private": true,
  "scripts": {
    "setup": "npx prisma migrate dev --name init",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "ts-standard --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "generate": "generate-library ./dist/fake.module @alecmmiller/interface-generator",
    "generateVersion": "semantic-release --branches feat/generators --dry-run --no-ci"
  },
  "dependencies": {
    "@nestjs-addons/in-memory-db": "^3.0.3",
    "@nestjs/common": "^9.0.0",
    "@nestjs/config": "^3.0.0",
    "@nestjs/core": "^9.0.0",
    "@nestjs/microservices": "^10.0.5",
    "@nestjs/platform-express": "^9.0.0",
    "@nestjs/swagger": "^7.1.8",
    "@prisma/client": "^4.16.2",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "dgram": "^1.0.1",
    "helmet": "^7.0.0",
    "mqtt": "^4.3.7",
    "mqtt-connection": "^4.1.0",
    "pigeon-mqtt-nest": "^1.0.17",
    "reflect-metadata": "^0.1.13",
    "regex": "^0.1.1",
    "rxjs": "^7.2.0"
  },
  "devDependencies": {
    "@alecmmiller/interface-generator": "^1.2.0",
    "@alecmmiller/nestjs-client-generator": "^1.4.2",
    "@nestjs/cli": "^9.0.0",
    "@nestjs/schematics": "^9.0.0",
    "@nestjs/testing": "^9.0.0",
    "@types/express": "^4.17.13",
    "@types/jest": "29.5.1",
    "@types/node": "20.7.0",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "29.5.0",
    "jest-sonar": "^0.2.16",
    "prettier": "^2.3.2",
    "prisma": "^4.16.2",
    "semantic-release": "^21.1.1",
    "source-map-support": "^0.5.20",
    "supertest": "^6.1.3",
    "ts-jest": "29.1.0",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "ts-standard": "^12.0.2",
    "tsconfig-paths": "4.2.0",
    "typescript": "^5.0.0"
  },
  "release": {
    "branches": [
      "beta"
    ],
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      [
        "@semantic-release/npm",
        {
          "npmPublish": false
        }
      ],
      "@semantic-release/github"
    ]
  }
}
