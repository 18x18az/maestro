{
  "name": "@18x18az/maestro",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "license": "MIT",
  "repository": "https://github.com/18x18az/maestro",
  "private": true,
  "scripts": {
    "setup": "npx prisma migrate dev --name init",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "ts-standard",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "generate": "generate-library ./dist/fake.module @alecmmiller/interface-generator",
    "generateVersion": "semantic-release --branches feat/generators --dry-run --no-ci",
    "prepare": "husky install"
  },
  "dependencies": {
    "@alecmmiller/pigeon-mqtt-nest": "^0.0.1",
    "@nestjs-addons/in-memory-db": "^3.0.3",
    "@nestjs/axios": "^3.0.0",
    "@nestjs/common": "^10.2.6",
    "@nestjs/config": "^3.1.1",
    "@nestjs/core": "^10.2.6",
    "@nestjs/jwt": "^10.1.1",
    "@nestjs/microservices": "^10.2.6",
    "@nestjs/passport": "^10.0.2",
    "@nestjs/platform-express": "^10.2.6",
    "@nestjs/schedule": "^4.0.0",
    "@nestjs/swagger": "^7.1.8",
    "@prisma/client": "^4.16.2",
    "axios": "^1.6.0",
    "bcrypt": "^5.1.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "dgram": "^1.0.1",
    "helmet": "^7.0.0",
    "mqtt": "^5.3.3",
    "mqtt-connection": "^4.1.0",
    "nestjs-real-ip": "^3.0.1",
    "node-html-parser": "^6.1.10",
    "obs-websocket-js": "^5.0.3",
    "passport": "^0.6.0",
    "passport-local": "^1.0.0",
    "reflect-metadata": "^0.1.13",
    "regex": "^0.1.1",
    "rxjs": "^7.2.0"
  },
  "devDependencies": {
    "@alecmmiller/interface-generator": "^1.2.2",
    "@alecmmiller/nestjs-client-generator": "^1.4.5",
    "@nestjs/cli": "^10.0.0",
    "@nestjs/schematics": "^10.0.0",
    "@nestjs/testing": "^10.0.0",
    "@types/express": "^4.17.18",
    "@types/jest": "29.5.5",
    "@types/multer": "^1.4.9",
    "@types/node": "20.8.6",
    "@types/passport-local": "^1.0.36",
    "@types/supertest": "^2.0.15",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.62.0",
    "eslint": "^8.51.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^8.0.0",
    "jest": "29.7.0",
    "jest-sonar": "^0.2.16",
    "prisma": "^4.16.2",
    "semantic-release": "^22.0.7",
    "source-map-support": "^0.5.20",
    "supertest": "^6.1.3",
    "ts-jest": "29.1.1",
    "ts-loader": "^9.5.0",
    "ts-node": "^10.0.0",
    "ts-standard": "^12.0.2",
    "tsconfig-paths": "4.2.0",
    "typescript": "^5.0.0"
  },
  "release": {
    "branches": [
      "beta"
    ],
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      [
        "@semantic-release/npm",
        {
          "npmPublish": false
        }
      ],
      "@semantic-release/github"
    ]
  }
}
